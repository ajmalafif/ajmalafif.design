<header class="w-100 fixed-ns left-0 ph4-ns left-0 top-0 right-0 z-2 bb b--light-gray bg-white o-90">
  <nav role="navigation" aria-label="main-navigation" class="dt w-100 border-box pv1 wrap">
    <div class="dib w-100 w-25-ns tl-ns tc">
      <h1 class="f5 mb0 mt3">
      <a class="dark-gray link logo fw6" href="/" title="Home">@ajmalafif</a>
      </h1>
    </div>
    <ul class="dib w-100 w-75-ns tr-ns tc mt3 mt0-ns">
      {%- for nav in collections.nav | sort(attribute="date") -%}
      <li class="f5 dib mr4"><a class="link dark-gray" href="{{ nav.url | url }}"{% if nav.url in page.url %} data-current="current"{% endif %}>{{ nav.data.navtitle }}</a></li>
      {%- endfor -%}
    </ul>
  </nav>
</header>
